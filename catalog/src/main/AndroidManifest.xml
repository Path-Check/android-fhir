<?xml version="1.0" encoding="utf-8" ?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.android.fhir.catalog"
>

  <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_card_file_box"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_card_file_box_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Androidfhir"
    >
    <activity android:exported="true" android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:exported="true" android:name=".CustomActivity">
      <intent-filter>
        <action android:name="com.google.android.fhir.datacapture.INPUT" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
  </application>

</manifest>
